<urn:a87ff679-a2f3-371d-9181-a67b7542122c> {
    ex:p1   rdf:type      ex:Person ;
            ex:firstName  "Albert" ;
            ex:lastName   "Einstein" .
}

<urn:eccbc87e-4b5c-32fe-a830-8fd9f2a7baf3> {
    ex:firstNameVariableShape
            rdf:type       sh:NodeShape ;
            sh:property    [ sh:datatype  xsd:string ;
                             sh:path      bl:boundTo
                           ] ;
            sh:targetNode  var:firstName .

    bl:onlyOneBoundValueShape
            rdf:type        sh:NodeShape ;
            sh:property     [ sh:maxCount  1 ;
                              sh:minCount  1 ;
                              sh:path      bl:boundTo
                            ] ;
            sh:targetClass  bl:Variable .

    ex:lastNameShape  rdf:type  sh:NodeShape ;
            sh:property    [ sh:datatype  xsd:string ;
                             sh:path      bl:boundTo
                           ] ;
            sh:targetNode  var:lastName .

    ex:PersonShape  rdf:type  sh:NodeShape ;
            sh:property     [ sh:datatype  xsd:string ;
                              sh:maxCount  1 ;
                              sh:minCount  1 ;
                              sh:path      ex:lastName
                            ] ;
            sh:property     [ sh:datatype  xsd:string ;
                              sh:maxCount  1 ;
                              sh:minCount  1 ;
                              sh:path      ex:firstName
                            ] ;
            sh:targetClass  ex:Person .
}

<urn:c4ca4238-a0b9-3382-8dcc-509a6f75849b> {
    var:firstName  rdf:type  bl:Variable ;
            bl:name   "firstName" .

    var:lastName  rdf:type  bl:Variable ;
            bl:name   "lastName" .

    ex:p1   rdf:type  bl:Unblendable .
}

<urn:c81e728d-9d4c-3f63-af06-7f89cc14862c> {
    var:lastName  bl:boundTo  "Einstein" .

    var:firstName  bl:boundTo  "Albert" .
}

