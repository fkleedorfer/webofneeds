    <!-- https://w3id.org/won/message#atom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#atom">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Atom"/>
        <rdfs:comment xml:lang="en">Links the message to its atom. Only used for messages directed at an atom, such as msg:Activate, msg:Deactivate, or msg:AtomHintMessage.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>atom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#connection -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#connection">
        <rdfs:domain rdf:resource="https://w3id.org/won/message#Message"/>
        <rdfs:range rdf:resource="https://w3id.org/won/core#Connection"/>
        <rdfs:comment xml:lang="en">Links the message to its connection. Only used for response messages in a connection; the messages sent by either side of the connection use msg:senderSocket and msg:recipientSocket to identify the connection uniquely</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>connection</rdfs:label>
    </owl:ObjectProperty>
    


