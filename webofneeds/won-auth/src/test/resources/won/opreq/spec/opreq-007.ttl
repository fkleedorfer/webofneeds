ex:atom1
    a                    won:Atom ;
    won:socket           atom1:chatSocket ;
    won:state            won:Active ;
    won:messageContainer atom1:msg ;
    won:wonNode          ex:node .

atom1:chatSocket
    a                    won:Socket ;
    won:socketDefinition chat:ChatSocket ;
    won:connections      atom1:chatSocketConnections .

atom1:chatSocketConnections
    rdfs:member atom1:bsConn12, atom1:bsConn13 .

atom1:bsConn12
    a                   won:Connection ;
    won:targetAtom      ex:atom2 ;
    won:connectionState won:Connected ;
    won:socket          atom1:chatSocket .

atom1:bsConn13
    a                   won:Connection ;
    won:targetAtom      ex:atom3 ;
    won:socket          atom3:chatSocket ;
    won:connectionState won:Connected .

ex:atom2
    a                    won:Atom ;
    won:socket           atom2:chatSocket ;
    won:state            won:Active ;
    won:messageContainer atom2:msg ;
    won:wonNode          ex:node .

atom2:chatSocket
    a                    won:Socket ;
    won:socketDefinition chat:ChatSocket ;
    won:connections      atom2:chatSocketConnections .

atom2:chatSocketConnections
    rdfs:member atom2:bsConn2 .

atom2:bsConn2
    a                   won:Connection ;
    won:targetAtom      ex:atom1 ;
    won:socket          atom2:chatSocket ;
    won:connectionState won:Connected .

ex:atom3
    a                    won:Atom ;
    won:socket           atom3:chatSocket ;
    won:state            won:Active ;
    won:messageContainer atom3:msg ;
    won:wonNode          ex:node .

atom3:chatSocket
    a                    won:Socket ;
    won:socketDefinition chat:ChatSocket ;
    won:connections      atom3:chatSocketConnections .

atom3:chatSocketConnections
    rdfs:member atom3:bsConn3 .

atom3:bsConn3
    a                   won:Connection ;
    won:targetAtom      ex:atom1 ;
    won:socket          atom3:chatSocket ;
    won:connectionState won:Connected .