ex:test-main {
    ex:PatternTransferAtLocation a bl:Template ;
    bl:dataGraph ex:test-data ;
}

ex:test-data {

    ex:Bob
        a foaf:Agent .

    ex:Alice
        a foaf:Agent .

    ex:alicesLawnmower1
            a vf:EconomicResource;
            vf:primaryAccountable ex:Alice ;
            vf:custodian ex:Alice ;
            vf:classifiedAs wd:Q260521 ;
            vf:currentLocation wd:Q676832 ;
            vf:onhandQuantity [
                om2:hasUnit om2:one ;
                om2:hasNumericalValue "0"^^xsd:int ;
            ] ;
            vf:accountingQuantity [
                om2:hasUnit om2:one ;
                om2:hasNumericalValue "0"^^xsd:int ;
            ]
            .

    ex:bobsLawnmower1
        a vf:EconomicResource;
        vf:primaryAccountable ex:Bob ;
        vf:custodian ex:Bob ;
        vf:classifiedAs wd:Q260521 ;
        vf:currentLocation wd:Q676832 ;
        vf:onhandQuantity [
            om2:hasUnit om2:one ;
            om2:hasNumericalValue "1"^^xsd:int ;
        ] ;
        vf:accountingQuantity [
            om2:hasUnit om2:one ;
            om2:hasNumericalValue "1"^^xsd:int ;
        ]
        .
