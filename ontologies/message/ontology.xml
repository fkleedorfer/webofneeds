        <rdfs:range rdf:resource="https://w3id.org/won/message#MessageType"/>
        <rdfs:comment xml:lang="en">Indicates the type of the message.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/won/message"/>
        <rdfs:label>messageType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#previousMessage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#previousMessage">
