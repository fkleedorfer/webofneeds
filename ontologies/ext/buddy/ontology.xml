    <!-- https://w3id.org/won/ext/buddy#BuddySocket -->


    <owl:NamedIndividual rdf:about="https://w3id.org/won/ext/buddy#BuddySocket">
        <rdf:type rdf:resource="https://w3id.org/won/core#SocketDefinition"/>
        <core:compatibleSocketDefinition rdf:resource="https://w3id.org/won/ext/buddy#BuddySocket"/>
        <core:derivesAtomProperty rdf:resource="https://w3id.org/won/ext/buddy#buddy"/>
        <rdfs:comment xml:lang="en">Compatible with other BuddySockets, derives the buddy relationship. Installs local
                                   authorizations that &lt;br&gt;

                                   a) gives buddies access to atoms on the https://w3id.org/won/ext/hold#HolderSocket
                                   by granting a token with scope https://w3id.org/won/ext/hold#ScopeReadHeldAtoms. &lt;br&gt;

                                   b) gives buddies access to the connections to other buddies using
                                   https://w3id.org/won/ext/buddy#ScopeReadConnectToBuddyOfBuddy</rdfs:comment>
        <rdfs:label>BuddySocket</rdfs:label>
        <auth:localAuth rdf:resource="https://w3id.org/won/ext/buddy#grantAccessToBuddies"/>
        <auth:localAuth rdf:resource="https://w3id.org/won/ext/buddy#grantReadConnectToBuddyOfBuddy"/>
    </owl:NamedIndividual>
    


