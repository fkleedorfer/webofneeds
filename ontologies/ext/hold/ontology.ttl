###  https://w3id.org/won/ext/hold#HoldableSocket
:HoldableSocket rdf:type owl:NamedIndividual ,
                         <https://w3id.org/won/core#SocketDefinition> ;
                <https://w3id.org/won/core#compatibleSocketDefinition> :HolderSocket ;
                <https://w3id.org/won/core#derivesAtomProperty> :heldBy ;
                <https://w3id.org/won/core#autoOpen> "false"^^xsd:boolean ;
                <https://w3id.org/won/core#socketCapacity> 1 ;
                rdfs:comment "An Atom uses a Socket with this configuration to connect to the Persona Atom that holds it."@en ;
                rdfs:label "HoldableSocket" ;
                <https://w3id.org/won/auth#localAuth> :grantReadViaHolderAuth .


###  https://w3id.org/won/ext/hold#HolderSocket
:HolderSocket rdf:type owl:NamedIndividual ,
                       <https://w3id.org/won/core#SocketDefinition> ;
              <https://w3id.org/won/core#compatibleSocketDefinition> :HoldableSocket ;
              <https://w3id.org/won/core#derivesAtomProperty> :holds ;
              <https://w3id.org/won/core#autoOpen> "false"^^xsd:boolean ;
              rdfs:comment "A Persona Atom uses a Socket with this configuration to connect to Atoms it holds."@en ;
              rdfs:label "HolderSocket" .


###  https://w3id.org/won/ext/hold#ScopeReadHeldAtoms
:ScopeReadHeldAtoms rdf:type owl:NamedIndividual ,
                             <https://w3id.org/won/auth#TokenScope> ;
                    rdfs:comment """Tokens with this scope, signed by the holder of an atom, provide read/connect
               access to the holder's atoms (see https://w3id.org/won/ext/hold#HoldableSocket)."""@en ;
                    rdfs:label "ScopeReadHeldAtoms" .


###  https://w3id.org/won/ext/hold#grantReadViaHolderAuth
:grantReadViaHolderAuth rdf:type owl:NamedIndividual ,
                                 <https://w3id.org/won/auth#Authorization> ;
                        <https://w3id.org/won/auth#bearer> _:genid1 ;
                        <https://w3id.org/won/auth#grant> [ <https://w3id.org/won/auth#graph> _:genid7
                                                          ] .

_:genid1 <https://w3id.org/won/auth#issuer> [ <https://w3id.org/won/auth#socket> _:genid3
                                             ] ;
          <https://w3id.org/won/auth#tokenScope> :ScopeReadHeldAtoms .

_:genid7 <https://w3id.org/won/auth#graphType> <https://w3id.org/won/auth#contentGraph> ,
                                                <https://w3id.org/won/auth#keyGraph> ,
                                                <https://w3id.org/won/auth#sysinfoGraph> ;
          <https://w3id.org/won/auth#operation> <https://w3id.org/won/auth#opRead> .

_:genid3 <https://w3id.org/won/auth#connection> _:genid4 ;
          <https://w3id.org/won/auth#socketType> :HoldableSocket .

_:genid4 <https://w3id.org/won/auth#connectionState> <https://w3id.org/won/core#Connected> ;
          <https://w3id.org/won/auth#targetAtom> [ ] .


###  https://w3id.org/won/ext/hold#heldBy
:heldBy rdf:type owl:NamedIndividual .


###  https://w3id.org/won/ext/hold#holds
:holds rdf:type owl:NamedIndividual .


#################################################################
#    Annotations
#################################################################

:heldBy rdfs:comment "Links an Atom to the Atom representing the agent (a person, organization, bot, ...) that controls both."@en ;
        rdfs:label "heldBy" .


:holds rdfs:comment "Links the Atom representing an agent (a person, organization, bot, ...) to another Atom they control."@en ;
       rdfs:label "holds" .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :HoldableSocket
                        :HolderSocket
                      )
] .


