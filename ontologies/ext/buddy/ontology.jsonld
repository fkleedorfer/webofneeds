  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://purl.org/dc/elements/1.1/abstract" : [ {
    "@language" : "en",
    "@value" : "This ontology contains the definition of the BuddySocket, which, when used to establish a connection, derives a *buddy* relationship between the Atoms."
  } ],
  "http://purl.org/dc/elements/1.1/creator" : [ {
    "@value" : "Florian Kleedorfer"
  } ],
  "http://purl.org/dc/elements/1.1/title" : [ {
    "@value" : "Web of Needs Buddy Extension"
  } ],
  "http://purl.org/dc/terms/created" : [ {
    "@type" : "http://www.w3.org/2001/XMLSchema#date",
    "@value" : "2019-04-15"
  } ],
  "http://purl.org/dc/terms/license" : [ {
    "@id" : "https://creativecommons.org/licenses/by/4.0/"
  } ],
  "http://purl.org/vocab/vann/preferredNamespacePrefix" : [ {
    "@value" : "wx-buddy"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "WoN Buddy Extension for establishing buddy relationships."
  } ],
  "http://www.w3.org/2002/07/owl#imports" : [ {
    "@id" : "https://w3id.org/won/core"
  } ],
  "http://www.w3.org/2002/07/owl#versionIRI" : [ {
    "@id" : "https://w3id.org/won/ext/buddy/1.0"
  } ]
}, {
  "@id" : "https://w3id.org/won/ext/buddy#BuddySocket",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "https://w3id.org/won/core#SocketDefinition" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Compatible with other BuddySockets, derives the buddy relationship. Installs local\n                                   authorizations that <br>\n\n                                   a) gives buddies access to atoms on the https://w3id.org/won/ext/hold#HolderSocket\n                                   by granting a token with scope https://w3id.org/won/ext/hold#ScopeReadHeldAtoms. <br>\n\n                                   b) gives buddies access to the connections to other buddies using\n                                   https://w3id.org/won/ext/buddy#ScopeReadConnectToBuddyOfBuddy"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "BuddySocket"
  } ],
  "https://w3id.org/won/auth#localAuth" : [ {
    "@id" : "https://w3id.org/won/ext/buddy#grantAccessToBuddies"
  }, {
    "@id" : "https://w3id.org/won/ext/buddy#grantReadConnectToBuddyOfBuddy"
  } ],
  "https://w3id.org/won/core#compatibleSocketDefinition" : [ {
    "@id" : "https://w3id.org/won/ext/buddy#BuddySocket"
  } ],
  "https://w3id.org/won/core#derivesAtomProperty" : [ {
    "@id" : "https://w3id.org/won/ext/buddy#buddy"
  } ]
}, {
  "@id" : "https://w3id.org/won/ext/buddy#ScopeReadConnectToBuddyOfBuddy",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "https://w3id.org/won/auth#TokenScope" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Scope for tokens that a) atoms grant their buddies, b) give access to the atom's\n    content and the list of buddies if signed by a buddy."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "ScopeReadConnectToBuddyOfBuddy"
  } ]
}, {
  "@id" : "https://w3id.org/won/ext/buddy#buddy",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Links a buddy Atom to one of its buddies. This should generally imply natural\n                       persons on both ends. We interpret buddies are people who like to do things\n                       together; such as engaging in sports or other social activities."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/won/core#Atom"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
