        <rdfs:comment xml:lang="en">Links a message to its envelope.</rdfs:comment>
        <rdfs:label>envelope</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#hintTargetAtom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#hintTargetAtom">
        <rdfs:comment xml:lang="en">The target Atom of the AtomHintMessage. By sending this message, the sender wants to express that the Atom&apos;s owner may want to establish a connection with the targetAtom (but first must choose a suitable pair of sockets), or that the targetAtom may just be interesting for the owner for other reasons.</rdfs:comment>
        <rdfs:label>hintTargetAtom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/won/message#hintTargetSocket -->


    <owl:ObjectProperty rdf:about="https://w3id.org/won/message#hintTargetSocket">
        <rdfs:comment xml:lang="en">The target Socket of a SocketHintMessage. By sending this message it is suggested to establish a connection between the recipientSocket and the targetSocket.</rdfs:comment>
        <rdfs:label>hintTargetSocket</rdfs:label>
    </owl:ObjectProperty>
    


